% 0. Provided commands

% 1. Primary commands
\newcommand{\Ccd}{Coccidioidomycosis} % [sng] Coccidioidomycosis
\newcommand{\Cim}{\textit{C.~immitis}} % [sng] C. immitis
\newcommand{\anmclmsbs}{\ensuremath{\prime}} % [sbs] Climatological Anomaly subscript
\newcommand{\anmcsnsbs}{\ensuremath{\prime\prime}} % [sbs] Climatological Seasonal Anomaly subscript
\newcommand{\crcsbs}{\ensuremath{*}} % [sbs] Corrected-for-Autoregression subscript
\newcommand{\ccd}{coccidioidomycosis} % [sng] coccidioidomycosis
% NB: Replace \blankidx with \mthidx in *mth variables in order to
% display a left-superscripted m in front of  anomalies from the
% climatological annual cycle (i.e., climatological monthly anomalies)
% These superscripts were removed in review since the defining
% appendices were also removed
\newcommand{\blankidx}{\ensuremath{}} % [idx] Blank index
\newcommand{\mthidx}{\ensuremath{m}} % [idx] Month index
\newcommand{\mthnbr}{\ensuremath{M}} % [nbr] Number of months m in time-series data
\newcommand{\ncd}{\ensuremath{N}} % [# s-1] Incidence rate, absolute
\newcommand{\tmidx}{\ensuremath{n}} % [idx] Time index
\newcommand{\tmnbr}{\ensuremath{N}} % [nbr] Number of time-series data
\newcommand{\yridx}{\ensuremath{y}} % [idx] Year index

% 2. Derived commands
\newcommand{\anmcsncrcsbs}{\crcsbs} % [sbs] Climatological Seasonal Anomaly Corrected-for-Autoregression (i.e., Level Three data) subscript
\newcommand{\flxpcpavg}{\ensuremath{\bar{\flxpcp}}} % [kg m2 s-1] Precipitation mass flux, climatological monthly mean
\newcommand{\flxpcpsvn}{\ensuremath{\flxpcp_{\Sigma 7}}} % [kg m2 s-1] Precipitation mass flux, cumulative seven-month
\newcommand{\ncdabs}{\ensuremath{\ncd_{0}}} % [# s-1] Incidence rate, absolute,
\newcommand{\ncdavg}{\ensuremath{\bar{\ncd}}} % [# s-1] Incidence rate, climatological mean
\newcommand{\ncdnrm}{\ensuremath{\bar{\ncd}}} % [# s-1 ppl-1] Incidence rate normalized per unit population
\newcommand{\prssfcavg}{\ensuremath{\bar{\prssfc}}} % [Pa] Pressure at surface, climatological mean
\newcommand{\tptsfcavg}{\ensuremath{\bar{\tptsfc}}} % [K] Temperature at surface, climatological mean
\newcommand{\xxxanmclm}{\ensuremath{\xxx^{\anmclmsbs}}} % [frc] Climatological anomaly
\newcommand{\xxxanmcsn}{\ensuremath{\xxx^{\anmcsnsbs}}} % [frc] Climatological seasonal anomaly
\newcommand{\xxxavgmth}{\ensuremath{^{\blankidx}\xxxavg}} % [frc] Climatological monthly mean
\newcommand{\xxxcrc}{\ensuremath{\xxx^{\crcsbs}}} % [frc] Climatological anomaly, auto-regression-corrected
\newcommand{\xxxidx}{\ensuremath{\xxx_{\tmidx}}} % [frc] Climatological anomaly, explicit timeseries subscript

